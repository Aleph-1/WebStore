<body>
    <div id="template1" class="header">
        <%- include ('../HomePage/HomePage.ejs') %>
    </div>


   <form method="post">
    <table class="container form-box" style="margin:0 auto;">
        <tr>
            <td></td>
            <td><h3 style="margin-top: 6rem;">Make A Product!</h3></td>
        </tr>

        <tr>
            <td>Title:</td>
            <td>
                <input type="text" placeholder="Enter Title:" name="title">
            </td>
        </tr>

        <tr>
            <td>Price:</td>
            <td>
                <input type="text" placeholder="Enter Price" name="price">
            </td>

        </tr>

        <tr>
            <td>Description:</td>
            <td>
                <input type="text" placeholder="Enter Description" name="Description">
            </td>

        </tr>

        <tr>
            <td>Short Description:</td>
            <td>
                <input type="text" placeholder="Enter Short Description" name="shortDescription">
            </td>

        </tr>
        
        <tr>
            <td>Amount Available:</td>
            <td>
                <input type="text" placeholder="Enter Amount Available" name="amountAvailable">
            </td>

        </tr>

        <tr>
            <td>Pictures</td>
            <td>
                <input type="text" placeholder="links seperated by a space" name="pictures">
            </td>

        </tr>

        <tr>
            <td>Sizes</td>
            <td>
                <input type="text" placeholder="sizes seperated by a  " name="Sizes">
            </td>

        </tr>

        <tr>
            <td>Tags:</td>
            <td>
                <input type="text" placeholder="Enter Tags" name="tags">
            </td>

        </tr>
        <tr>
            <td></td>
            <td>
                <button style="width: 20rem;" class="btn btn-primary" type="submit">Submit</button>
            </td>
        </tr>
    </table>


   </form>


</body>


</html>